{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run vscode-bootstrap Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}/packages/vscode-bootstrap"],
      "outFiles": ["${workspaceFolder}/packages/vscode-bootstrap/dist/**/*.js"],
      "preLaunchTask": "npm: build:vscode:watch"
    }
  ]
}
